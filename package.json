{
  "name": "family-cordinator",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "start": "node --env-file=.env --experimental-strip-types src/index.ts",
    "build": "tsc",
    "migrate": "tsx --env-file=.env src/db/migrate.ts",
    "test": "vitest run",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": "prettier --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Family calendar coordinator via Signal",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "date-fns": "^4.1.0",
    "googleapis": "^171.4.0",
    "luxon": "^3.7.2",
    "pg": "^8.18.0",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "signal-sdk": "^0.1.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.2.3",
    "@types/pg": "^8.16.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
